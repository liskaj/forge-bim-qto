    "@types/express": "^4.0.36",
    "@types/jquery": "^3.2.16",
    "body-parser": "^1.15.2",
    "bower": "^1.8.2",
    "express": "^4.14.0",
    "forge-apis": "^0.4.1",
    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.3",
